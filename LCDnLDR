
/* LCD Pins
 * RS: Pin 22
 * EN: Pin 23
 * D4: Pin 24
 * D5: Pin 25
 * D6: Pin 26
 * D7: Pin 27
 */
 
#include <LiquidCrystal.h>

LiquidCrystal lcd(22,23,24,25,26,27);
int bright = 65;

 // LDR Setup
 int LDR1_Pin = A0;
 int LDR2_Pin = A1;
 int LDR3_Pin = A2;
 int LDR4_Pin = A3;
 int LDR5_Pin = A4;

void setup() {
  Serial.begin(9600);
}

void loop() {
  int LDR1 = analogRead(LDR1_Pin); 
  int LDR2 = analogRead(LDR2_Pin);
  int LDR3 = analogRead(LDR3_Pin);
  int LDR4 = analogRead(LDR4_Pin);
  int LDR5 = analogRead(LDR5_Pin);
  
  Serial.println(LDR1);
  Serial.println(LDR2);
  Serial.println(LDR3);
  Serial.println(LDR4);
  Serial.println(LDR5);  
  delay(250);  
}
